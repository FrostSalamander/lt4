///////////////////// roundtop_white01 /////////////////////////

textures/darkmod/window/roundtop_white01/roundtop_white01_barelytlit
{
	glass
	noselfshadow

	qer_editorimage textures/darkmod/window/roundtop_white01_barelylit_ed
	diffusemap textures/darkmod/window/roundtop_white01
	{
		blend add
		map 	textures/darkmod/window/roundtop_white01_pane_lit
		rgb 0.3
	}
	bumpmap 	textures/darkmod/window/roundtop_white01_local
	specularmap textures/darkmod/window/roundtop_white01_s

}

